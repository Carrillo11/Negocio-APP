<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_margin="0dp"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tvfecha"
        android:layout_width="135dp"
        android:layout_height="15sp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/lato"
        android:text="Fecha"
        android:textSize="15sp"
        android:textStyle="bold" />

    <LinearLayout
        android:layout_width="85dp"
        android:layout_height="22dp"
        android:layout_marginLeft="290dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/contenedor_radius">

        <TextView
            android:id="@+id/tvproveedor"
            android:layout_width="80dp"
            android:layout_height="13sp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="3dp"
            android:fontFamily="@font/lato"
            android:text="Proveedor"
            android:textColor="#FFFFFF"
            android:textSize="12sp"
            android:textStyle="bold" />

    </LinearLayout>
    <TextView
        android:id="@+id/tvproducto"
        android:layout_width="80dp"
        android:layout_height="15sp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="65dp"
        android:fontFamily="@font/lato"
        android:text="Producto"
        android:textSize="13sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tvpreciounit"
        android:layout_width="80dp"
        android:layout_height="15sp"
        android:layout_marginLeft="303dp"
        android:layout_marginTop="65dp"
        android:fontFamily="@font/lato_black"
        android:text="Precio"
        android:textColor="#04DF00"
        android:textSize="14sp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="15dp"
        android:layout_height="15sp"
        android:layout_marginLeft="293.5dp"
        android:layout_marginTop="65dp"
        android:fontFamily="@font/lato_black"
        android:text="$"
        android:textColor="#04DF00"
        android:textSize="14sp"
        android:textStyle="bold" />



    <LinearLayout
        android:layout_width="340dp"
        android:layout_height="3dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="95dp"
        android:background="@drawable/separador"></LinearLayout>

</RelativeLayout>